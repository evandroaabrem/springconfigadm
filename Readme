https://github.com/evandroaabrem/springconfigadm


curl.exe -X PUT http://localhost:8081/github/dev `
  -H "Content-Type: text/plain" `
  --data-binary "server.port=9090`n`nspring.datasource.url=jdbc:mysql://localhost/dev`n`nspring.datasource.username=dev_user`n`nspring.datasource.password=dev_pass`nfeature.flag.novaFuncionalidade=true"
